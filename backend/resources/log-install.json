{
  "root_loggers": [
    {
      "levels": [
        "TRACE",
        "DEBUG",
        "INFO",
        "WARN",
        "ERROR"
      ],
      "format": {
        "format": "{{chrono(TIME)}} {{module({})}} {{level({})}}: {{message({})}}",
        "placeholders": {
          "TIME": {
            "format": "%Y-%m-%d %H:%M:%S"
          }
        }
      },
      "targets": [
        {
          "type": "file-logger",
          "properties": {
            "file": "install.log"
          }
        }
      ]
    }
  ]
}